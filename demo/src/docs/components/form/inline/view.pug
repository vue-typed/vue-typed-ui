ui-form(v-on:success='submit', label-width='100px', :validator='validator')
  ui-input(kind='inline', name="firstName", label='First Name', placeholder='Please enter first name', v-model='firstName')
  ui-input(kind='inline', name="lastName", label='Last Name', placeholder='Please enter last name', v-model='lastName')
  ui-button.positive(type="submit") Submit
  ui-message.error
