{
  "base": {
    "FieldBase": "../../fields/field-base"
  },
  "props": {
    "name": {
      "type": "string",
      "description": "Input element name"
    },
    "placeholder": {
      "type": "string",
      "description": "Input element placeholder"
    },
    "value": {
      "type": "any",
      "description": "v-model binding"
    },
    "password": {
      "type": "boolean",
      "description": "Input element is password element"
    },
    "icon": {
      "type": "string",
      "description": "Name of icon to be displayed"
    },
    "icon-pos": {
      "type": "'left' | 'right' | string",
      "description": "Icon position",
      "default": "'left'"
    },
    "css": {
      "type": "string",
      "description": "Css class(es) applied to input component"
    },
    "select-on-focus": {
      "type": "boolean",
      "default": "true",
      "description": "Select text when receive focus"
    }
  },
  "methods": {
    "target": {
      "type": "JQuery",
      "description": "Target element"
    }
  },
  "events": {
    "change": {
      "description": "Callback when input has been change"
    }
  }
}